<div class="page--fund-center" style="--fund-color: {{ fund_color }}">
  <div class="organism--secondary-nav">
    <nav class="secondary-nav">
      <div class="secondary-nav-wrapper grid-container">
        <div class="nav-header">
          <span class="page-title">{{ fund_name }}</span>
          <ul class="navigation menu">
            <li>
              <a href="#fund-overview" rel="nofollow" class="">{{ 'Overview'|t }}</a>
            </li> {% if product_type != 'tst' %} <li>
              <a href="#fund-performance" rel="nofollow" class="">{{ 'Performance'|t }}</a>
            </li> {% endif %} <li>
              <a href="#fund-keyfacts" rel="nofollow" class="">{{ 'Key Facts'|t }}</a>
            </li>
            <li>
              <a href="#fund-characteristics" rel="nofollow" class="">{{ 'Characteristics'|t }}</a>
            </li>
            <li>
              <a href="#fund-allocations" rel="nofollow" class="">{{ 'Allocations'|t }}</a>
            </li> {% if product_type == 'etf' %} <li>
              <a href="#fund-pricing" rel="nofollow" class="">{{ 'Pricing'|t }}</a>
            </li> {% endif %} {% if explorer_type == 'GMM' %} <li>
              <a href="#fund-moneymarket" rel="nofollow" class="">{{ 'Money Market'|t }}</a>
            </li> {% endif %} <li>
              <a href="#fund-documents" rel="nofollow" class="">{{ 'Documents'|t }}</a>
            </li>
          </ul>
        </div>
        <span class="active-marker"></span>
      </div>
    </nav>
  </div>
